
<apex:page standardController="Candidate__c" lightningStylesheets="True">
    <apex:form>
        <apex:pageBlock title="Candidate Info" mode="edit">
            <apex:pageBlockButtons>
                <apex:commandButton action="{!save}" value="Save"/>
                <apex:commandButton action="{!cancel}" value="Cancel"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="Enter Your Info" columns="2">
                <apex:inputField value="{!Candidate__c.Name}"/>
                <apex:inputField value="{!Candidate__c.Branch__c}"/>
                <apex:inputField value="{!Candidate__c.CGPA__c}"/>
                <apex:inputField value="{!Candidate__c.College__c}"/>
                <apex:inputField value="{!Candidate__c.Passout_Year__c}"/>
                <apex:inputField value="{!Candidate__c.Date_Of_Birth__c}"/>
                <apex:inputField value="{!Candidate__c.Email__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>